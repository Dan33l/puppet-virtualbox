{
  "name": "puppet-virtualbox",
  "version": "4.0.2-rc0",
  "author": "Vox Pupuli",
  "summary": "Installs Virtualbox on RHEL and Debian-like systems",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-virtualbox",
  "project_page": "https://github.com/voxpupuli/puppet-virtualbox",
  "issues_url": "https://github.com/voxpupuli/puppet-virtualbox/issues",
  "tags": [
    "virtualbox",
    "virtualization"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.1.0 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 9.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.1.0 < 9.0.0"
    },
    {
      "name": "puppet/epel",
      "version_requirement": ">= 3.0.0 <4.0.0"
    }
  ]
}
